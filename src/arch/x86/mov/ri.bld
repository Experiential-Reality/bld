# ri.bld - MOV r64, imm64 instruction
#
# MovRI IS: rex(1 byte) + opcode(1 byte) + immediate(8 bytes)

structure MovRI

D output: 10 [byte]

B parts: prefix | opcode | immediate
  prefix -> rex(W=1, B=dst.ext)
  opcode -> mov_ri_base + dst.lo
  immediate -> imm

L compose: parts -> output (deps=1)

returns: output
