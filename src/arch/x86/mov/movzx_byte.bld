# movzx_byte.bld - MOVZX r32, [r64] (byte source)
#
# MovzxByte IS: escape + opcode + modrm

structure MovzxByte

D output: 3 [byte]

B parts: escape | opcode | modrm
  escape -> arch/x86/opcodes/escape
  opcode -> arch/x86/opcodes/movzx(byte)
  modrm -> arch/x86/modrm(mod=0, reg=dst.lo, rm=src.lo)

L compose: parts -> output (deps=1)

returns: output
