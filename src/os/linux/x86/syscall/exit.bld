# exit.bld - Linux exit syscall for x86-64
#
# SyscallExit IS: mov_ri * 2 + syscall

structure SyscallExit

D output: 22 [byte]

B parts: rax | rdi | syscall
  rax -> arch/x86/mov/ri(rax, os/linux/syscall/exit)
  rdi -> arch/x86/mov/ri(rdi, code)
  syscall -> arch/x86/syscall()

L compose: parts -> output (deps=1)

returns: output
