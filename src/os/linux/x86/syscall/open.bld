# open.bld - Linux open syscall for x86-64
#
# SyscallOpen IS: mov_ri * 4 + syscall

structure SyscallOpen

D output: 42 [byte]

B parts: rax | rdi | rsi | rdx | syscall
  rax -> arch/x86/mov/ri(rax, os/linux/syscall/open)
  rdi -> arch/x86/mov/ri(rdi, path)
  rsi -> arch/x86/mov/ri(rsi, flags)
  rdx -> arch/x86/mov/ri(rdx, mode)
  syscall -> arch/x86/syscall()

L compose: parts -> output (deps=1)

returns: output
