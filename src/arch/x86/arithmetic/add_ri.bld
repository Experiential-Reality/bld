# add_ri.bld - ADD r64, imm8 instruction
#
# AddRI IS: rex + opcode + modrm + imm

structure AddRI

D output: 4 [byte]

B parts: rex | opcode | modrm | imm
  rex -> rex(W=1)
  opcode -> arch/x86/opcodes/add_mi8
  modrm -> arch/x86/modrm(mod=3, reg=0, rm=dst.lo)
  imm -> byte(imm)

L compose: parts -> output (deps=1)

returns: output
