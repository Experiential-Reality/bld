# inc.bld - INC r64 instruction
#
# Inc IS: rex + opcode + modrm

structure Inc

D output: 3 [byte]

B parts: rex | opcode | modrm
  rex -> rex(W=1, B=dst.ext)
  opcode -> arch/x86/opcodes/inc_dec
  modrm -> arch/x86/modrm(mod=3, reg=0, rm=dst.lo)

L compose: parts -> output (deps=1)

returns: output
