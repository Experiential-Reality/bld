# write.bld - Linux write syscall for x86-64
#
# SyscallWrite IS: mov_ri * 4 + syscall

structure SyscallWrite

D output: 42 [byte]

B parts: rax | rdi | rsi | rdx | syscall
  rax -> arch/x86/mov/ri(rax, os/linux/syscall/write)
  rdi -> arch/x86/mov/ri(rdi, fd)
  rsi -> arch/x86/mov/ri(rsi, buf)
  rdx -> arch/x86/mov/ri(rdx, len)
  syscall -> arch/x86/syscall()

L compose: parts -> output (deps=1)

returns: output
