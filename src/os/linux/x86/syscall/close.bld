# close.bld - Linux close syscall for x86-64
#
# SyscallClose IS: mov_ri * 2 + syscall

structure SyscallClose

D output: 22 [byte]

B parts: rax | rdi | syscall
  rax -> arch/x86/mov/ri(rax, os/linux/syscall/close)
  rdi -> arch/x86/mov/ri(rdi, fd)
  syscall -> arch/x86/syscall()

L compose: parts -> output (deps=1)

returns: output
